{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5433;Database=loghub;Username=loghub_user;Password=loghub_pass"
  },
  "RabbitMQ": {
    "Host": "localhost",
    "Port": "5672",
    "Username": "loghub",
    "Password": "loghub_pass"
  },
  "Redis": {
    "Configuration": "localhost:6380"
  },
  "Jwt": {
    "Key": "LogHubSuperSecretKey2024ForJwtAuthentication!AtLeast32Chars",
    "Issuer": "LogHub",
    "Audience": "LogHubDashboard",
    "ExpiryHours": "24"
  },
  "Dashboard": {
    "Url": "http://localhost:4200"
  },
  "LogRetention": {
    "Days": 30
  }
}
